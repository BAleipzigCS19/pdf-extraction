<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="800.0" prefWidth="1050.0" stylesheets="@css/ViewStylesheet.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.baleipzig.pdfextraction.client.controller.CreateTemplateController">
    <top>
        <fx:include fx:id="menuBar" source="MenuBar.fxml" />
    </top>
    <center>
      <GridPane prefHeight="772.5" prefWidth="1050.0">
          <columnConstraints>
              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <rowConstraints>
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <children>
              <AnchorPane fx:id="pdfAnchor">
                  <children>
                      <fx:include fx:id="pdfPreview" source="PdfPreview.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="5.0" />
                  </children>
               <GridPane.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </GridPane.margin>
              </AnchorPane>
              <GridPane fx:id="dataGridPane" GridPane.columnIndex="1">
                  <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  </columnConstraints>
                  <rowConstraints>
                      <RowConstraints maxHeight="143.0" minHeight="120.0" prefHeight="143.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="30.0" minHeight="17.0" prefHeight="17.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  </rowConstraints>
                  <children>
                      <GridPane prefHeight="120.0" prefWidth="290.0">
                          <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                              <RowConstraints maxHeight="120.0" minHeight="120.0" prefHeight="120.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                          <children>
                              <VBox prefHeight="200.0" prefWidth="100.0">
                                  <children>
                                      <Label styleClass="header" text="Versicherung">
                                 <VBox.margin>
                                    <Insets bottom="5.0" top="10.0" />
                                 </VBox.margin></Label>
                                      <TextField fx:id="insuranceTextField" />
                                      <Label styleClass="header" text="Templatename">
                                          <VBox.margin>
                                              <Insets bottom="5.0" top="20.0" />
                                          </VBox.margin>
                                      </Label>
                                      <TextField fx:id="templateNameTextField" />
                                  </children>
                              </VBox>
                          </children>
                      </GridPane>
                      <JFXButton id="addField" fx:id="addFieldButton" alignment="CENTER" mnemonicParsing="false" onAction="#addFieldButtonOnClick" styleClass="button-blue" text="+" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
                     <GridPane.margin>
                        <Insets right="150.0" />
                     </GridPane.margin></JFXButton>
                      <Label styleClass="header" text="Daten Ermitteln" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
                      <JFXButton fx:id="createTemplateButton" mnemonicParsing="false" onAction="#createTemplateButtonOnAction" styleClass="button-blue" text="Template speichern" GridPane.halignment="RIGHT" GridPane.rowIndex="3" GridPane.valignment="BOTTOM" />
                      <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonOnAction" styleClass="button-white" text="Abbrechen" GridPane.halignment="LEFT" GridPane.rowIndex="3" GridPane.valignment="BOTTOM" />
                      <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
                          <children>
                              <GridPane fx:id="datagrid" vgap="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
                                  <columnConstraints>
                                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="125.0" minWidth="10.0" prefWidth="111.0" />
                                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="237.0" minWidth="10.0" prefWidth="237.0" />
                                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="155.0" minWidth="10.0" prefWidth="102.0" />
                                  </columnConstraints>
                                  <rowConstraints>
                                      <RowConstraints minHeight="10.0" prefHeight="30.0" />
                                  </rowConstraints>
                              </GridPane>
                          </children>
                      </AnchorPane>
                  </children>
                  <GridPane.margin>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </GridPane.margin>
              </GridPane>
          </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </GridPane>
   </center>
</BorderPane>
