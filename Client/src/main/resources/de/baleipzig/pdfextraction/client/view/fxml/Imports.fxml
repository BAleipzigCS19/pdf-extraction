<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane stylesheets="@../css/ViewStylesheet.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.baleipzig.pdfextraction.client.controller.ImportController">
   <top>
       <fx:include fx:id="menuBar" source="MenuBar.fxml" />
   </top>
    <center>
      <GridPane prefHeight="772.5" prefWidth="1050.0">
          <columnConstraints>
              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <rowConstraints>
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <children>
              <AnchorPane>
                  <children>
                      <fx:include fx:id="pdfPreview" source="PdfPreview.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                  </children>
              </AnchorPane>
              <GridPane prefHeight="481.0" prefWidth="363.0" GridPane.columnIndex="1">
                  <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" />
                  </columnConstraints>
                  <rowConstraints>
                      <RowConstraints />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  </rowConstraints>
                  <children>
                      <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                          <children>
                              <VBox>
                                  <children>
                                      <Label styleClass="header" text="%templatesHeader" />
                                      <JFXComboBox fx:id="templateComboBox" prefHeight="25.0" prefWidth="160.0" promptText="%templateComboBox" styleClass="check-box">
                                          <VBox.margin>
                                              <Insets top="10.0" />
                                          </VBox.margin>
                                      </JFXComboBox>
                                  </children>
                              </VBox>
                          </children>
                          <GridPane.margin>
                              <Insets />
                          </GridPane.margin>
                      </AnchorPane>
                      <ScrollPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
                          <content>
                              <Label styleClass="header" text="%collectedDataHeader">
                                  <padding>
                                      <Insets bottom="10.0" />
                                  </padding>
                              </Label>
                          </content>
                      </ScrollPane>
                      <JFXButton fx:id="continueButton" mnemonicParsing="false" onAction="#continueButtonOnAction" styleClass="button-blue" text="%continueButton" GridPane.halignment="RIGHT" GridPane.rowIndex="3" GridPane.valignment="BOTTOM" />
                  </children>
                  <padding>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
              </GridPane>
          </children>
      </GridPane>
   </center>
</BorderPane>
