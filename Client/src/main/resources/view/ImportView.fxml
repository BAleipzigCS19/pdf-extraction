<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<SplitPane dividerPositions="0.5" prefHeight="454.0" prefWidth="604.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.baleipzig.pdfextraction.controller.ImportController">
   <items>
      <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="452.0" prefWidth="304.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="350.0" prefWidth="298.0">
               <children>
                  <ImageView fx:id="imageView" layoutX="4.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
               </children>
            </AnchorPane>
          <AnchorPane maxHeight="50.0" minHeight="50.0" minWidth="0.0" prefHeight="50.0" prefWidth="298.0">
               <children>
                  <Button fx:id="buttonPageForward" layoutX="254.0" layoutY="13.0" mnemonicParsing="false" onAction="#onClickPageForward" text="-&gt;" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0" />
                  <Button fx:id="buttonPageBack" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onAction="#onClickPageBack" text="&lt;-" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="10.0" />
                  <Label fx:id="pageIndex" alignment="CENTER" layoutX="122.0" layoutY="17.0" text="Seitenzahl" AnchorPane.bottomAnchor="16.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
      <GridPane hgap="10.0" prefHeight="400.0" prefWidth="600.0" vgap="10.0">
          <columnConstraints>
              <ColumnConstraints hgrow="SOMETIMES" />
          </columnConstraints>
          <rowConstraints>
              <RowConstraints />
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <children>
              <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                  <children>
                      <VBox>
                          <children>
                              <Label text="Templates" />
                              <ComboBox fx:id="templateComboBox" prefWidth="200.0" promptText="Template auswählen">
                                  <VBox.margin>
                                      <Insets top="10.0" />
                                  </VBox.margin>
                              </ComboBox>
                          </children>
                      </VBox>
                  </children>
                  <GridPane.margin>
                      <Insets />
                  </GridPane.margin>
              </AnchorPane>
              <ScrollPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
                  <content>
                      <Label text="Ermittelte Daten">
                          <padding>
                              <Insets bottom="10.0" />
                          </padding>
                      </Label>
                  </content>
              </ScrollPane>
            <GridPane GridPane.rowIndex="3">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
               <children>
                    <Button fx:id="continueButton" mnemonicParsing="false" onAction="#continueButtonOnAction" text="Weiter" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.valignment="BOTTOM" />
                  <Button fx:id="buttonChooseFile" mnemonicParsing="false" onAction="#onClickChooseFile" text="Datei Auswählen" />
               </children>
               <rowConstraints>
                  <RowConstraints />
               </rowConstraints>
            </GridPane>
          </children>
          <padding>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
      </GridPane>
   </items>
</SplitPane>
